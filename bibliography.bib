@article{Dalloglio1995,
title = "Interstellar dust spectral index: a balloon-borne experiment",
journal = "Planetary and Space Science",
volume = "43",
number = "10",
pages = "1389 - 1393",
year = "1995",
note = "Dust, Molecules and Backgrounds: from Laboratory to Space",
issn = "0032-0633",
doi = "https://doi.org/10.1016/0032-0633(95)00055-A",
url = "http://www.sciencedirect.com/science/article/pii/003206339500055A",
author = "G. Dall'Oglio and G. Morgante and L. Pizzo and L. Valenziano and P. Merluzzi",
abstract = "A balloon-borne experiment has been conceived in order to map the sub-millimetric galactic emission at four different wavelengths between 400 μm and 2 mm at an angular scale of 1°. Such measurements will make it possible both to investigate the nature of the interstellar dust and to disentangle the galactic contribution from extragalactic and cosmological signals. A binocular telescope system has been realized by means of two 37 cm of-axis wobbling parabolic mirrors, each one coupled with a two-channel 3He photometer. The first trans-Mediterranean flight is scheduled for summer 1995."
}

